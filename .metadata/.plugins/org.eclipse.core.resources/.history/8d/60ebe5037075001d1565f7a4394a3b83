public interface CartDao {

	// 장바구니 추가
	public int addCart(CartDTO cart) throws Exception;
	
	// 장바구니 삭제
	public void deleteCart(int cNum) throws Exception;
	
	// 장바구니 수량 변경
	public int addCart(CartDTO cart) throws Exception;
	
	// 장바구니 목록
	public int addCart(CartDTO cart) throws Exception;
	
}
